{
  "name": "11ty-sass-skeleton",
  "version": "1.4.0",
  "description": "Featuring absolutely nothing beyond a base HTML5 template and the essential setup to watch and compile your Sass alongside 11ty.",
  "main": "index.html",
  "scripts": {
    "watch:tailwind": "npx postcss src/_styles/style.css --o src/_includes/assets/css/style.css --watch",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "concurrently \"npm run watch:tailwind\" \"npm run watch:eleventy\"",
    "build": "npm-run-all build:sass build:eleventy",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5t3ph/11ty-sass-skeleton.git"
  },
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.17",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "sass": "^1.49.7"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ],
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "luxon": "^2.3.1",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.23"
  }
}